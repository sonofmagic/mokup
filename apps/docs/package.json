{
  "name": "docs",
  "type": "module",
  "version": "0.0.3",
  "private": true,
  "homepage": "https://mokup.icebreaker.top",
  "scripts": {
    "dev": "vitepress dev docs --host",
    "prebuild:docs": "pnpm --filter mokup build",
    "build:docs": "vitepress build docs",
    "build:playground": "MOKUP_PLAYGROUND_SW=1 MOKUP_PLAYGROUND_OUT_DIR=dist-docs pnpm --filter @mokup/playground build && MOKUP_PLAYGROUND_OUT_DIR=dist-docs node scripts/copy-playground.mjs",
    "build:routes": "node scripts/build-playground-routes.mjs",
    "build": "pnpm build:docs && pnpm build:playground && pnpm build:routes",
    "preview": "vitepress preview docs",
    "worker:dev": "pnpm build && wrangler dev",
    "worker:deploy": "pnpm build && wrangler deploy"
  },
  "dependencies": {
    "@faker-js/faker": "catalog:"
  },
  "devDependencies": {
    "@tailwindcss/vite": "catalog:",
    "mokup": "workspace:*",
    "tailwindcss": "catalog:",
    "vite": "catalog:",
    "vitepress": "^1.6.3",
    "vitepress-plugin-group-icons": "^1.7.1",
    "vitepress-plugin-llms": "^1.10.0",
    "wrangler": "catalog:"
  }
}
