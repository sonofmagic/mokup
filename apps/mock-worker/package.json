{
  "name": "mock-worker",
  "type": "module",
  "version": "0.0.5",
  "private": true,
  "homepage": "https://mokup.icebreaker.top",
  "scripts": {
    "build:manifest": "pnpm --filter mokup build && pnpm exec mokup build --dir mock --out src/.mokup",
    "dev": "wrangler dev src/index.ts",
    "deploy": "wrangler deploy src/index.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "hono": "^4.9.0",
    "mokup": "workspace:*"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260118.0",
    "typescript": "^5.9.3",
    "wrangler": "^4.0.0"
  }
}
