{
  "name": "mock-worker",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build:manifest": "pnpm --filter @mokup/cli build && mokup build --dir mock --out src/.mokup",
    "dev": "wrangler dev src/index.ts",
    "deploy": "wrangler deploy src/index.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@mokup/server": "workspace:*",
    "hono": "^4.9.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260117.0",
    "@mokup/cli": "workspace:*",
    "@mokup/runtime": "workspace:*",
    "typescript": "^5.9.3",
    "wrangler": "^4.0.0"
  }
}
