{
  "name": "mock-worker",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build:manifest": "pnpm --filter moku-cli build && moku build --dir mock --out src/.moku",
    "dev": "wrangler dev src/index.ts",
    "deploy": "wrangler deploy src/index.ts",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "hono": "^4.9.0",
    "moku-hono": "workspace:*"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250912.0",
    "moku-cli": "workspace:*",
    "typescript": "^5.9.3",
    "wrangler": "^4.0.0"
  }
}
