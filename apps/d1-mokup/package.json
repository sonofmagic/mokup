{
  "name": "d1-mokup",
  "type": "module",
  "version": "0.0.3",
  "private": true,
  "homepage": "https://mokup.icebreaker.top",
  "scripts": {
    "dev": "vite --host",
    "prebuild": "pnpm --filter mokup build",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "preworker:deploy": "pnpm --filter mokup build",
    "worker:deploy": "vite build && wrangler deploy"
  },
  "dependencies": {
    "better-auth": "^1.3.4",
    "drizzle-orm": "^0.45.1",
    "mokup": "workspace:*",
    "vue": "catalog:"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.21.2",
    "@cloudflare/workers-types": "^4.20260124.0",
    "@tailwindcss/vite": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "drizzle-kit": "^0.31.5",
    "tailwindcss": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "wrangler": "catalog:"
  }
}
