{
  "name": "web",
  "type": "module",
  "version": "0.0.5",
  "private": true,
  "homepage": "https://mokup.icebreaker.top",
  "scripts": {
    "dev": "vite --host",
    "build:manifest": "pnpm --filter mokup build && pnpm exec mokup build --dir mock --out worker/src/.mokup",
    "worker:dev": "pnpm build:manifest && wrangler dev",
    "worker:deploy": "pnpm build:manifest && wrangler deploy",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "mokup": "workspace:*",
    "vue": "^3.5.26",
    "vue-router": "^4.6.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.13",
    "@vitejs/plugin-vue": "^6.0.3",
    "tailwindcss": "^4.1.13",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "wrangler": "^4.0.0"
  }
}
