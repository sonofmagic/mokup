{
  "name": "web",
  "type": "module",
  "version": "0.0.12",
  "private": true,
  "homepage": "https://mokup.icebreaker.top",
  "scripts": {
    "dev": "vite --host",
    "build:manifest": "pnpm --filter mokup build && pnpm exec mokup build --dir mock --out worker/src/.mokup",
    "worker:dev": "pnpm build:manifest && wrangler dev",
    "worker:deploy": "pnpm build:manifest && wrangler deploy",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@faker-js/faker": "catalog:",
    "axios": "^1.13.2",
    "mokup": "workspace:*",
    "vue": "catalog:",
    "vue-router": "^4.6.0"
  },
  "devDependencies": {
    "@tailwindcss/vite": "catalog:",
    "@vitejs/plugin-vue": "catalog:",
    "tailwindcss": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:",
    "wrangler": "catalog:"
  }
}
