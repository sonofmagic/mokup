{
  "name": "@mokup/cli",
  "type": "module",
  "version": "1.0.11",
  "description": "CLI for building mokup manifests and handlers.",
  "license": "MIT",
  "homepage": "https://mokup.icebreaker.top",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sonofmagic/mokup.git",
    "directory": "packages/cli"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@mokup/runtime": "workspace:*",
    "@mokup/server": "workspace:*",
    "@mokup/shared": "workspace:*",
    "commander": "^14.0.0"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "unbuild": "catalog:"
  }
}
