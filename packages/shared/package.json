  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "chokidar": "^5.0.0",
    "consola": "^3.4.2",
    "esbuild": "^0.27.2",
    "hono": "^4.11.7",
    "jsonc-parser": "^3.3.1",
