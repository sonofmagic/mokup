{
  "name": "mokup",
  "type": "module",
  "version": "0.2.0",
  "description": "Mock utilities and Vite plugin for mokup.",
  "license": "MIT",
  "homepage": "https://mokup.icebreaker.top",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sonofmagic/mokup.git",
    "directory": "packages/mokup"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.mjs",
      "require": "./dist/cli.cjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.mjs",
      "require": "./dist/server.cjs"
    },
    "./server/worker": {
      "types": "./dist/server/worker.d.ts",
      "import": "./dist/server/worker.mjs",
      "require": "./dist/server/worker.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.cjs"
    },
    "./runtime": {
      "types": "./dist/runtime.d.ts",
      "import": "./dist/runtime.mjs",
      "require": "./dist/runtime.cjs"
    },
    "./sw": {
      "types": "./dist/sw.d.ts",
      "import": "./dist/sw.mjs",
      "require": "./dist/sw.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "mokup": "./dist/cli-bin.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0 || ^7.0.0"
  },
  "dependencies": {
    "@mokup/cli": "workspace:*",
    "@mokup/playground": "workspace:*",
    "@mokup/runtime": "workspace:*",
    "@mokup/server": "workspace:*",
    "chokidar": "^5.0.0",
    "esbuild": "^0.27.2",
    "hono": "^4.11.4",
    "jsonc-parser": "^3.3.1",
    "pathe": "^2.0.3"
  },
  "devDependencies": {
    "@types/node": "^25.0.9",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "vite": "^7.3.1"
  }
}
