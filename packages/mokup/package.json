{
  "name": "mokup",
  "type": "module",
  "version": "2.0.0",
  "description": "Mock utilities and Vite plugin for mokup.",
  "license": "MIT",
  "homepage": "https://mokup.icebreaker.top",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sonofmagic/mokup.git",
    "directory": "packages/mokup"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./cli": {
      "types": "./dist/cli.d.ts",
      "import": "./dist/cli.mjs",
      "require": "./dist/cli.cjs"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.mjs",
      "require": "./dist/server.cjs"
    },
    "./server/worker": {
      "types": "./dist/server/worker.d.ts",
      "import": "./dist/server/worker.mjs",
      "require": "./dist/server/worker.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "types": "./dist/webpack.d.ts",
      "import": "./dist/webpack.mjs",
      "require": "./dist/webpack.cjs"
    },
    "./runtime": {
      "types": "./dist/runtime.d.ts",
      "import": "./dist/runtime.mjs",
      "require": "./dist/runtime.cjs"
    },
    "./sw": {
      "types": "./dist/sw.d.ts",
      "import": "./dist/sw.mjs",
      "require": "./dist/sw.cjs"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "mokup": "./dist/cli-bin.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "peerDependencies": {
    "html-webpack-plugin": "^5.0.0",
    "vite": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "webpack": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "html-webpack-plugin": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "@mokup/cli": "workspace:*",
    "@mokup/playground": "workspace:*",
    "@mokup/runtime": "workspace:*",
    "@mokup/server": "workspace:*",
    "@mokup/shared": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "vite": "catalog:"
  }
}
